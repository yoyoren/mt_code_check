{
  "name": "mt_fe_codecheck",
  "version": "1.0.3",
  "description": "code style check for mt fe",
  "main": "install.js",
  "scripts": {
     "coverage": "istanbul cover ./node_modules/.bin/_mocha -- test.js",
     "example-fail": "echo \"This is the example hook, I exit with 1\" && exit 1",
     "example-pass": "echo \"This is the example hook, I exit with 0\" && exit 0",
     "install": "node install.js",
     "test": "mocha test.js",
     "test-travis": "istanbul cover node_modules/.bin/_mocha --report lcovonly -- test.js",
     "uninstall": "node uninstall.js"
  },

  "repository": {
    "type": "git",
    "url": "git@github.com:yoyoren/mt_code_check.git"
  },

  "dependencies": {
    "cross-spawn": "2.0.x",
    "which": "1.2.x"
  },
  
  "devDependencies": {
    "assume": "1.3.x",
    "istanbul": "0.4.x",
    "mocha": "2.3.x",
    "mt_fe_codecheck": "git@github.com:yoyoren/mt_code_check.git"
  },
  "keywords": [
    "code",
    "check"
  ],
  "author": "renyuan",
  "license": "MIT"
}
